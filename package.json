{
  "name": "erp-microservico",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
"scripts": {
  "build:gateway": "nest build erp-api-gateway",
  "build:users": "nest build users",
  "build:product": "nest build product",
  "build:all": "concurrently \"pnpm:build:gateway\" \"pnpm:build:users\" \"pnpm:build:product\"",
  "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
  "start:dev:gateway": "nest start --watch erp-api-gateway",
  "start:dev:users": "nest start --watch users",
  "start:dev:product": "nest start --watch product",
  "start:dev:all": "concurrently \"pnpm:start:dev:gateway\" \"pnpm:start:dev:users\" \"pnpm:start:dev:product\"",
  "start:debug:gateway": "nest start --debug --watch erp-api-gateway",
  "start:debug:users": "nest start --debug --watch users",
  "start:debug:product": "nest start --debug --watch product",
  "start:debug:all": "concurrently \"pnpm:start:debug:gateway\" \"pnpm:start:debug:users\" \"pnpm:start:debug:product\"",
  "start:prod:gateway": "node dist/apps/erp-microservico/main",
  "start:prod:users": "node dist/apps/users/main",
  "start:prod:product": "node dist/apps/product/main",
  "start:prod:all": "concurrently \"pnpm:start:prod:gateway\" \"pnpm:start:prod:users\" \"pnpm:start:prod:product\"",
  "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
  "test": "vitest run",
  "test:e2e": "vitest run --config ./vitest.config.e2e.ts"
},
  "dependencies": {
    "@nestjs/common": "^11.0.8",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.8",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.0.8",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.8",
    "@nestjs/testing": "^11.0.8",
    "bcryptjs": "^2.4.3",
    "decimal.js": "^10.5.0",
    "dotenv": "^16.4.7",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "zod": "^3.24.1",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.4.0",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/swagger": "^11.0.3",
    "@prisma/client": "^6.3.1",
    "@swc/core": "^1.10.15",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/node": "^20.17.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "@vitest/coverage-v8": "^3.0.5",
    "prettier": "^3.4.2",
    "prisma": "^6.3.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "unplugin-swc": "^1.5.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5"
  }
}
